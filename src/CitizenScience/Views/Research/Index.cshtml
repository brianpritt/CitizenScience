@model List<CitizenScience.Models.Fauna>
<script src="~/js/drawMap.js"></script>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBNmEdwEFOm_I2jY6WqN98p_QItuuYfF4s&callback=initMap"></script>
<style>
    #map {
        height: 400px;
        width: 600px;
    }
</style>
<div id="map"></div>
@foreach (CitizenScience.Models.Fauna item in Model)
{
    string photoImage = "";
    if (item.FaunaPhoto != null)
    {
        
        photoImage = String.Format("data:image/gif;base64," + Convert.ToBase64String(item.FaunaPhoto));
        <h3>its here</h3>
        
    }
    <h5>Name: @item.FaunaName Description: @item.FaunaDescripton</h5>
        <img src="@photoImage" alt="@item.FaunaPhoto.Length"/>
        <p>@item.FaunaPhoto</p>
}

